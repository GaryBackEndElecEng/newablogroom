{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@/components/*": [
        "./src/components/*"
      ],
      "@editor/*": [
        "./src/components/editor/*"
      ],
      "@/prisma/*": [
        "./prisma/*"
      ],
      "@/lib/*": [
        "./src/lib/*"
      ],
      "@/auth/*": [
        "./auth"
      ]
    },
    "strictNullChecks": true,
    "target": "ES2017"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "../deleted/savegetblog.tsx"
  ],
  "exclude": [
    "node_modules",
    "seed"
  ]
}